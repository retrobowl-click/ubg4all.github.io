{
  "manifest_version": 3,
  "name": "__MSG_AppName__",
  "version": "1.1.0",
  "version_name": "1.1.0",
  "action": {
    "default_icon": {
      "16": "img/16x16.png",
      "48": "img/48x48.png",
      "256": "img/256x256.png"
    },
    "default_title": "Click to open the popup",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "update.js"
  },

  "offline_enabled": true,
  "default_locale": "en",
  "description": "__MSG_AppDesc__",
  "icons": {
    "16": "img/16x16.png",
    "48": "img/48x48.png",
    "256": "img/256x256.png"
  },
  "options_page": "index.html",
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts; script-src 'self' 'unsafe-eval' blob: 'unsafe-inline'; object-src 'self' 'unsafe-eval' blob: 'unsafe-inline';child-src 'self' 'unsafe-eval' blob: 'unsafe-inline' ; worker-src 'self' 'unsafe-eval' blob: 'unsafe-inline'; script-src-elem 'self' 'unsafe-eval' blob: 'unsafe-inline'"
  },
  "sandbox": { "pages": ["game/index.html"] },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "exclude_matches": [],
      "js": ["game/cker.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "game/polyfill/attach.js",
        "game/polyfill/chrome/polyfill.min.js",
        "game/polyfill/edge/polyfill.min.js",
        "game/polyfill/firefox/polyfill.min.js"
      ],
      "matches": ["https://*/*"]
    }
  ],
  "permissions": ["alarms", "storage"],
  "homepage_url": "https://cookieclicker.ee"
}
